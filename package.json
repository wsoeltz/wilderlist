{
  "name": "notch-api",
  "version": "1.0.0",
  "description": "",
  "main": "dist/index.js",
  "engines": {
    "node": "11.11.0",
    "npm": "6.7.0"
  },
  "scripts": {
    "start": "node dist/index.js",
    "dev:start": "npm run build && node dist/index.js",
    "server": "node node_modules/nodemon/bin/nodemon.js",
    "client": "npm run start --prefix client",
    "build:server": "npm run clean && tsc",
    "build:client": "npm run build --prefix client",
    "build": "npm run build:server && npm run build:client",
    "clean": "rimraf dist",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "lint:fix": "tslint --project tsconfig.json --config tslint.json --fix",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/express": "^4.17.0",
    "@types/jest": "^24.0.15",
    "jest": "^24.7.1",
    "nodemon": "^1.19.1",
    "ts-jest": "^24.0.2",
    "ts-node": "^8.3.0",
    "tslint": "^5.18.0",
    "typescript": "^3.5.3",
    "typescript-styled-plugin": "^0.14.0"
  },
  "dependencies": {
    "axios": "^0.19.0",
    "concurrently": "^4.1.1",
    "express": "^4.17.1",
    "rimraf": "^2.6.3"
  }
}
